{
	"manifest_version": 3,
	"name": "AionTab",
	"version": "1.1.2",
	"description": "A custom start page with an analog & digital clock and search functionality.",
	"permissions": ["scripting", "storage", "webNavigation", "activeTab"],
	"chrome_url_overrides": {
		"newtab": "./aion_tab/index.html"
	},
	"browser_action": {
		"default_popup": "./aion_tab/index.html"
	},
	"icons": {
		"16": "./icons/aion.png",
		"48": "./icons/aion.png",
		"128": "./icons/aion.png"
	}
}
